{
  "name": "@saasfly/chadnext",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "prisma generate && bun with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "prisma generate && bun with-env next dev",
    "lint": "bun with-env next lint",
    "format": "prettier --write '**/*.{js,cjs,mjs,ts,tsx,md,json}' --ignore-path .prettierignore",
    "start": "bun with-env next start",
    "with-env": "dotenv -e .env.local --",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.4",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@prisma/client": "5.9.1",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@saasfly/api": "workspace:*",
    "@saasfly/auth": "workspace:*",
    "@saasfly/db": "workspace:*",
    "@saasfly/ui": "workspace:*",
    "@t3-oss/env-nextjs": "0.8.0",
    "lucide-react": "^0.460.0",
    "next": "15",
    "next-international": "^1.3.1",
    "next-themes": "0.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.53.2",
    "tailwindcss-animate": "1.0.7",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@saasfly/eslint-config": "workspace:*",
    "@saasfly/prettier-config": "workspace:*",
    "@saasfly/tailwind-config": "workspace:*",
    "@saasfly/typescript-config": "workspace:*",
    "@types/node": "20.12.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.17",
    "dotenv-cli": "7.3.0",
    "eslint": "8.57.0",
    "prettier": "3.2.5",
    "prisma": "5.9.1",
    "tailwindcss": "3.4.1",
    "typescript": "5.4.5"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@saasfly/eslint-config/base",
      "@saasfly/eslint-config/nextjs",
      "@saasfly/eslint-config/react"
    ]
  },
  "prettier": "@saasfly/prettier-config"
}
